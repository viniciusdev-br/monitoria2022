Algoritmo "USUARIO_QUE_MANDA_NESSA_P"

Var
// Seção de Declarações das variáveis 
   limite: inteiro
   i: inteiro
Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
   escreva("Digite o limite da contagem: ")
   leia(limite)
   Enquanto (i <= limite) faca
      escreva(i)
      i <- i + 1
   Fimenquanto
Fimalgoritmo